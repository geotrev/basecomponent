{
  "name": "upgraded-element",
  "version": "0.2.1",
  "description": "A simple base class for using native web components",
  "main": "lib/upgraded-element.cjs.js",
  "module": "lib/upgraded-element.esm.js",
  "browser": "lib/upgraded-element.js",
  "scripts": {
    "start": "rollup -c rollup.config.demo.js -w",
    "build": "rm -rf lib && rollup -c",
    "build:publish": "rm -rf lib && NODE_ENV='publish' rollup -c",
    "lint": "eslint . --ext .js -c .eslintrc.js --ignore-path .eslintignore",
    "prepublishOnly": "npm run build:publish"
  },
  "homepage": "https://github.com/geotrev/upgraded-element",
  "author": "George Treviranus",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.5",
    "rollup": "^2.21.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^6.1.0"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "javascript",
    "js",
    "frontend",
    "web",
    "components",
    "web components"
  ],
  "files": [
    "lib/**.{js,map}",
    "src/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geotrev/upgraded-element.git"
  },
  "bugs": {
    "url": "https://github.com/geotrev/upgraded-element/issues"
  }
}
